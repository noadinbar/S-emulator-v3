<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.opening.table.history.HistoryController">
    <children>
        <TableView fx:id="tblHistory" prefHeight="180.0" styleClass="history-root">
            <columns>
                <TableColumn fx:id="colRunId" prefWidth="60.0" text="#Run" />
                <TableColumn fx:id="colDegree" prefWidth="80.0" text="Degree" />
                <TableColumn fx:id="colInputs" prefWidth="220.0" text="Inputs" />
                <TableColumn fx:id="colY" prefWidth="100.0" text="Result(y)" />
                <TableColumn fx:id="colCycles" prefWidth="90.0" text="Cycles" />
            </columns>
        </TableView>
      <HBox prefHeight="29.0" prefWidth="553.0" spacing="10.0">
         <children>
            <Button fx:id="btnShow" mnemonicParsing="false" text="Show" />
            <Button fx:id="btnRerun" mnemonicParsing="false" text="Re-run" />
         </children>
         <padding>
            <Insets left="10.0" />
         </padding>
      </HBox>
    </children>
</VBox>
